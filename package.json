{
  "name": "ng2-typeahead",
  "version": "1.2.0",
  "description": "A simple Angular2 typeahead/autocomplete component with no third-party dependencies.",
  "main": "./lib/ng2-typeahead",
  "scripts": {
    "postinstall": "typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "demo.serve": "run-s tsc demo.build lite",
    "demo.build": "ng build -prod --aot",
    "localdev": "tsc && cp -r lib/ ../dev-site/node_modules/ng2-typeahead/ && cp -r src/ ../dev-site/node_modules/ng2-typeahead/"
  },
  "keywords": [
    "angular",
    "angular2",
    "typeahead",
    "angularjs",
    "autocomplete",
    "ng2",
    "ng"
  ],
  "author": "Joe Brinkman <brinkmanjg@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brinkmanjg/ng2-typeahead.git"
  },
  "bugs": {
    "url": "https://github.com/brinkmanjg/ng2-typeahead/issues"
  },
  "homepage": "https://github.com/brinkmanjg/ng2-typeahead",
  "typescript": {
    "definition": "index.d.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "^1.1.0",
    "@angular/common": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.26",
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.3.4",
    "typings": "^1.3.2"
  }
}
